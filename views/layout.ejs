    <!-- start: Header -->
    <div class="navbar">
      <div class="navbar-inner">
        <div class="container-fluid">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="/"><span>Potato King</span></a>

          <!-- start: Header Menu -->
          <div class="nav-no-collapse header-nav">
            <ul class="nav pull-right">
              <!-- start: User Dropdown -->
              <li class="dropdown">
                <a class="btn dropdown-toggle userName" data-toggle="dropdown" href="#">
                  <i class="halflings-icon white user"></i>
                  <% if(typeof deviceIp !== 'undefined') { %>
                    <%= deviceIp.type %> | <%= deviceIp.device.table %>
                  <% } %>
                  <span class="caret"></span>
                </a>
                <ul class="dropdown-menu">
                  <li><a href="#"><i class="halflings-icon white user"></i> Profile</a></li>
                  <li><a href="login.html"><i class="halflings-icon white off"></i> Logout</a></li>
                </ul>
              </li>
              <!-- end: User Dropdown -->
            </ul>
          </div>
          <!-- end: Header Menu -->

        </div>
      </div>
    </div>
    <!-- start: Header -->

    <div class="container-fluid">
      <div class="row-fluid">


        <!-- start: Main Menu -->
        <div id="sidebar-left" class="span1">
          <div class="nav-collapse sidebar-nav">
            <ul class="nav nav-tabs nav-stacked main-menu">
              <% if(typeof deviceIp !== 'undefined') { %>
                <% if(deviceIp.type == 'guest') { %>
                  <li><a href="/"><i class="fa-icon-sitemap"></i><span class="hidden-tablet"> Thực Đơn</span></a></li>
                  <li>
                    <a href="/ordered"><i class="fa-icon-hdd"></i>
                      <span class="hidden-tablet">Thanh Toán
                        <!-- <span class="menu-notification green" id="numOfItem">0</span> -->
                      </span>
                    </a>
                  </li>
                <% } else if (deviceIp.type == 'cashier') { %>
                  <li><a href="/devices"><i class="fa-icon-sitemap"></i><span class="hidden-tablet"> Thiết Bị</span></a></li>
                  <li><a href="/ordered/overview"><i class="fa-icon-sitemap"></i><span class="hidden-tablet"> Bàn đang mở</span></a></li>
                <% } else if (deviceIp.type == 'chief-cook') { %>
                  <li><a href="/devices"><i class="fa-icon-sitemap"></i><span class="hidden-tablet"> Thiết Bị</span></a></li>
                  <li><a href="/ordered/overview"><i class="fa-icon-sitemap"></i><span class="hidden-tablet"> Bàn đang mở</span></a></li>
