<div id="divOrderedPageAlert">  
  
</div><!--/row-->

<div id="orderedPage" class="row-fluid">
  	<h1>Các món đã đặt</h1>

	<div class="box span11">
		<div class="box-content">
			<table class="table">
			  	<thead>
				  	<tr>
					  	<th>Món</th>
				  	</tr>
			  	</thead>   
			  	<tbody id="addedItemTableBody">
	  		  		<% _.each(ordered, function (item) { %>
	  		  			<tr class="dish<%= item.dish.id %>">
	  		  				<td>
	  		  					<%= item.dish.name %>
	  		  				</td>
	  		  			</tr>
	  		  		<% }) %>
			  	</tbody>
		  	</table>            
		</div>
		<div class="row-fluid device-list-footer">
		  <div class="device-list-footer-right">
		    <button onclick="checkout(<%= session.id %>)" class="btn btn-large btn-success" data-toggle="modal">Thanh toán</button>
		  </div>
		</div>
	</div>
	
	
</div>