<div id="divOrderedPageAlert">  
  
</div><!--/row-->

<div id="orderedPage" class="row-fluid">
  	<div class="page-title">
  		<img src="/img/template/title-ordered.png">
  	</div>

	<div class="box span11">
		<div>
			<table class="table">
			  	<thead>
				  	<tr class="table-header">
					  	<th>MÓN</th>
					  	<th>&nbsp;</th>
					  	<th>TRẠNG THÁI</th>
				  	</tr>
			  	</thead>   
			  	<tbody id="orderedItemTableBody">
	  		  		<% _.each(ordered, function (item) { %>
	  		  			<tr class="dish<%= item.dish.id %>">
	  		  				<td>
	  		  					<div class="item-name-vn"><%= item.dish.name %></div>
	  		  					<div class="item-name-en"><%= item.dish.nameEn %>................................................................</div>
	  		  				</td>
	  		  				<td>
	  		  					<div class="item-price">
	  		  						<%= item.price %>vnd
	  		  					</div>	  		  					
	  		  				</td>
	  		  				<td id="tdSessionDetail<%= item.id %>">
	  		  					<div class="item-status">
		  		  					<% if(item.status == 'added') { %>
										Đã chọn
									<% } else if(item.status == 'cancelled') { %>
										Đã xóa
		  		  					<% } else if(item.status == 'ordered') { %>
										Đã đặt								
									<% } else if(item.status == 'removed') { %>
										Nhà bếp đã hủy
									<% } else if(item.status == 'delivered') { %>
										Đã giao
		  		  					<% } else { %>
										<%= item.status %>
									<% } %>
	  		  					</div>
	  		  				</td>
	  		  			</tr>
	  		  		<% }) %>
			  	</tbody>
			  	<tfoot>
		  		  	<tr class="table-header">
		  			  	<th>TỔNG TIỀN</th>
		  			  	<th><%= session.total %>vnd</th>
		  			  	<th>&nbsp;</th>
		  		  	</tr>
			  	</tfoot>
		  	</table>            
		</div>
		<div class="row-fluid device-list-footer">
		  <div class="device-list-footer-right">
		    <!-- <button id="btnCheckout" onclick="checkout(<%= session.id %>)" class="btn btn-large btn-success" data-toggle="modal">Thanh toán</button> -->
		    <input type="image" id="btnCheckout" onclick="checkout(<%= session.id %>)" src="/img/template/btn-checkout.png">
		  </div>
		</div>
	</div>
	
	
</div>